<%= simple_form_for (@boat) do |f| %>
   <%= f.input :title %>
   <%= f.input :photo, as: :file %>
   <%= f.input :address %>
   <%= f.input :size %>
   <%= f.input :city %>
   <%= f.input :description %>
   <%= f.input :price %>
   <%= link_to "Back", :back, class: "btn btn-info" %>
   <%= f.submit "Submit", class: "btn btn-success" %>
 <% end %>
